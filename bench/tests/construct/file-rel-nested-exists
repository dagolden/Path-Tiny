{
    my $wd = tempd;
    my $path = "foo/bar/baz.txt";
    path($path)->touch;

    $result = timethese(
        $count,
        {
            'Path::Tiny'  => sub { path($path) },
            'Path::Class' => sub { file($path) },
            'IO::All'     => sub { io($path) },
            'File::Fu'    => sub { File::Fu->file($path) },
        },
        "none"
    );
}
